{
  "rules": {
    "indent": [
      2,
      4
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-console": 0,
    "global-strict": 0,
    "no-underscore-dangle": 0,
    "no-trailing-spaces": 0
  },
  "env": {
    "browser": true
  },
  "parser": "babel-eslint"
}
